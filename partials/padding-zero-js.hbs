<script>
    function paddingZero(cssSelector) {
        let el = document.querySelector(cssSelector);
        if (!el) {
            return;
        }

        let num = parseInt(el.innerText);
        if (!num) {
            return;
        }

        if (num > 0 && num < 10) {
            el.innerText = '0' + num;
        }
    }

    paddingZero('#pagination__current');
    paddingZero('#pagination__total');
</script>